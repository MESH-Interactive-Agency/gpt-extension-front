{
  "manifest_version": 3,
  "name": "GPT-3 Extension",
  "version": "1.0",
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["https://chat.openai.com/"],
  "action": {
    "default_popup": "index.html",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
